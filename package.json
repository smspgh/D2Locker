{
  "name": "D2Locker",
  "version": "2",
  "description": "Turkey forked D2L.",
  "main": "app/index.html",
  "private": true,
  "engines": {
    "node": ">=18",
    "pnpm": ">=8"
  },
  "type": "module",
  "scripts": {
    "//": "--- DOCKER COMMANDS ---",
    "BRS": "powershell.exe -File ./scripts/D2Locker_backup_rebuild.ps1",
    "dev": "pnpm util:light-data && concurrently --kill-others-on-fail \"pnpm serve:api\" \"pnpm dev:server\"",
    "prod": "concurrently \"pnpm serve:api\" \"pnpm util:light-data && pnpm build:prod --watch\" \"pnpm serve:hmr\"",
    "start": "pnpm build:prod && pnpm serve:prod",
    "dev-nf": "pnpm util:light-data-nf && concurrently --kill-others-on-fail \"pnpm serve:api\" \"pnpm dev:server\"",
    "prod-nf": "concurrently \"pnpm serve:api\" \"pnpm util:light-data-nf && pnpm build:prod --watch\" \"pnpm serve:hmr\"",
    "start-nf": "pnpm build:prod-nf && pnpm serve:prod-nf",
    "build": "webpack --config ./config/webpack.ts",
    "build:dev": "pnpm -s build --env=dev",
    "build:prod": "pnpm -s build --env=release --node-env=production",
    "build:beta": "pnpm -s build --env=beta --node-env=production",
    "build:pr": "pnpm -s build --env=pr --node-env=production",
    "serve:api": "pnpm --dir backend run api",
    "serve:hmr": "node simple-hmr-server.js",
    "serve:prod": "concurrently \"pnpm serve:api\" \"pnpm util:light-data-nf\" \"pnpm serve:hmr\"",
    "dev:server": "nodemon --delay 2 --watch config/webpack.ts,config/feature-flags.ts,config/content-security-policy.ts,pnpm-lock.yaml -e ts,js,json,lock --exec \"pnpm install --frozen-lockfile --prefer-offline && webpack serve --config ./config/webpack.ts --env=dev || pnpm util:touch-webpack\"",
    "lint": "pnpm run '/^lint:.*/'",
    "lint:eslint": "eslint --cache --cache-location .eslintcache --cache-strategy content src config backend *.js *.cjs --ignore-pattern 'node_modules' --no-warn-ignored",
    "lint:prettier": "prettier \"{src,config,backend,scripts}/**/*.{js,ts,tsx,cjs,mjs,cts,mts,scss}\" \"*.{js,ts,tsx,cjs,mjs,cts,mts}\" --check",
    "lint:stylelint": "stylelint \"src/**/*.scss\"",
    "lint:report": "pnpm lint:eslint --output-file eslint.results.json --format json",
    "fix": "pnpm run --sequential '/^fix:.*/'",
    "fix:eslint": "pnpm lint:eslint --fix",
    "fix:prettier": "prettier \"{src,config,backend,scripts}/**/*.{js,ts,tsx,cjs,mjs,cts,mts,scss}\" \"*.{js,ts,tsx,cjs,mjs,cts,mts}\" --write",
    "fix:stylelint": "pnpm lint:stylelint --fix",
    "fix:json": "jsonsort src/locale",
    "util:light-data-nf": "node scripts/download_light_json_process_trait_enhanced-mapping.js --no-fetch",
    "util:light-data": "node scripts/download_light_json_process_trait_enhanced-mapping.js",
    "util:touch-webpack": "node -e \"require('fs').utimes('config/webpack.ts', new Date(), new Date(), () => {})\"",
    "util:i18n": "i18next-scanner --config ./i18next-scanner.config.cjs",
    "util:i18n:watch": "nodemon --watch config/i18n.json --exec \"pnpm util:i18n\"",
    "util:dead-code": "ts-unused-exports tsconfig.json --allowUnusedTypes --allowUnusedEnums --showLineNumber | grep -v cssExports",
    "util:syntax-check": "find dist -name \"*.js\" | xargs -n 1 node --check",
    "util:fa-subset": "node src/fa-subset.js",
    "util:bcu": "node src/build-browsercheck-utils.js && prettier --write src/browsercheck-utils.js",
    "deps:check": "pnpm outdated --long",
    "deps:update": "pnpm up -i --latest",
    "deps:pnpm": "corepack use 'pnpm@*'",
    "deps:inspect-eslint": "pnpm dlx @eslint/config-inspector",
    "docker:build": "docker-compose build",
    "docker:build:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml build d2locker-dev",
    "docker:build:prod": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml build d2locker-prod",
    "docker:build:dev-nf": "docker-compose build --build-arg NO_FILTER=true d2locker-dev-nf",
    "docker:build:prod-nf": "docker-compose build --build-arg NO_FILTER=true d2locker-prod-nf",
    "docker:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up d2locker-dev",
    "docker:prod": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up d2locker-prod",
    "docker:dev-nf": "docker-compose up d2locker-dev-nf",
    "docker:prod-nf": "docker-compose up d2locker-prod-nf",
    "docker:api": "docker-compose up backend-api",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:shell": "docker-compose exec d2locker-dev sh",
    "docker:shell:prod": "docker-compose exec d2locker-prod sh",
    "docker:shell:dev-nf": "docker-compose exec d2locker-dev-nf sh",
    "docker:shell:prod-nf": "docker-compose exec d2locker-prod-nf sh",
    "docker:clean": "docker-compose down -v --rmi local"
  },
